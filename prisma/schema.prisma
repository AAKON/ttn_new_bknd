  updated_at           DateTime?              @db.Timestamp(0)
  model_has_roles      model_has_roles[]
  role_has_permissions role_has_permissions[]

  @@unique([name, guard_name], map: "roles_name_guard_name_unique")
}

model site_settings {
  id         BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  key        String    @unique(map: "site_settings_key_unique") @db.VarChar(255)
  value      String?   @db.Text
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)
}

model sourcing_proposals {
  id                                          BigInt                            @id @default(autoincrement()) @db.UnsignedBigInt
  user_id                                     BigInt                            @db.UnsignedBigInt
  product_category_id                         BigInt?                           @db.UnsignedBigInt
  location_id                                 BigInt?                           @db.UnsignedBigInt
  title                                       String                            @db.VarChar(255)
  description                                 String                            @db.Text
  quantity                                    Decimal?                          @db.Decimal(15, 2)
  unit                                        sourcing_proposals_unit?
  price                                       Decimal?                          @db.Decimal(15, 2)
  currency                                    sourcing_proposals_currency?
  payment_method                              sourcing_proposals_payment_method
  delivery_info                               String?                           @db.Text
  company_name                                String?                           @db.VarChar(255)
  company_slug                                String?                           @db.VarChar(255)
  email                                       String?                           @db.VarChar(255)
  phone                                       String?                           @db.VarChar(255)
  whatsapp                                    String?                           @db.VarChar(255)
  status                                      sourcing_proposals_status         @default(pending)
  admin_notes                                 String?                           @db.Text
  approved_by                                 BigInt?                           @db.UnsignedBigInt
  approved_at                                 DateTime?                         @db.Timestamp(0)
  view_count                                  Int                               @default(0)
  created_at                                  DateTime?                         @db.Timestamp(0)
  updated_at                                  DateTime?                         @db.Timestamp(0)
  deleted_at                                  DateTime?                         @db.Timestamp(0)
  favorite_proposals                          favorite_proposals[]
  proposal_comments                           proposal_comments[]
  users_sourcing_proposals_approved_byTousers users?                            @relation("sourcing_proposals_approved_byTousers", fields: [approved_by], references: [id], onUpdate: Restrict, map: "sourcing_proposals_approved_by_foreign")
  locations                                   locations?                        @relation(fields: [location_id], references: [id], onUpdate: Restrict, map: "sourcing_proposals_location_id_foreign")
  product_categories                          product_categories?               @relation(fields: [product_category_id], references: [id], onUpdate: Restrict, map: "sourcing_proposals_product_category_id_foreign")
  users_sourcing_proposals_user_idTousers     users                             @relation("sourcing_proposals_user_idTousers", fields: [user_id], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "sourcing_proposals_user_id_foreign")

  @@index([approved_by], map: "sourcing_proposals_approved_by_foreign")
  @@index([location_id], map: "sourcing_proposals_location_id_index")
  @@index([product_category_id], map: "sourcing_proposals_product_category_id_index")
  @@index([status, created_at], map: "sourcing_proposals_status_created_at_index")
  @@index([user_id], map: "sourcing_proposals_user_id_index")
}

model users {
  id                                                       BigInt                     @id @default(autoincrement()) @db.UnsignedBigInt
  uuid                                                     String?                    @unique(map: "users_uuid_unique") @db.Char(36)
  first_name                                               String                     @db.VarChar(255)
  last_name                                                String                     @db.VarChar(255)
  email                                                    String                     @unique(map: "users_email_unique") @db.VarChar(255)
  phone                                                    String?                    @db.VarChar(255)
  is_banned                                                Boolean                    @default(false)
  email_verified_at                                        DateTime?                  @db.Timestamp(0)
  password                                                 String                     @db.VarChar(255)
  status                                                   users_status               @default(pending)
  user_type                                                users_user_type            @default(user)
  remember_token                                           String?                    @db.VarChar(100)
  deleted_at                                               DateTime?                  @db.Timestamp(0)
  created_at                                               DateTime?                  @db.Timestamp(0)
  updated_at                                               DateTime?                  @db.Timestamp(0)
  companies_companies_created_byTousers                    companies[]                @relation("companies_created_byTousers")
  companies_companies_user_idTousers                       companies[]                @relation("companies_user_idTousers")
  company_claims                                           company_claims[]
  favorite_proposals                                       favorite_proposals[]
  proposal_comment_replies                                 proposal_comment_replies[]
  proposal_comments                                        proposal_comments[]
  sourcing_proposals_sourcing_proposals_approved_byTousers sourcing_proposals[]       @relation("sourcing_proposals_approved_byTousers")
  sourcing_proposals_sourcing_proposals_user_idTousers     sourcing_proposals[]       @relation("sourcing_proposals_user_idTousers")
}

