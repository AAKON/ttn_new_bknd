model proposal_comment_replies {
  id                  BigInt            @id @default(autoincrement()) @db.UnsignedBigInt
  proposal_comment_id BigInt            @db.UnsignedBigInt
  user_id             BigInt            @db.UnsignedBigInt
  reply               String            @db.Text
  created_at          DateTime?         @db.Timestamp(0)
  updated_at          DateTime?         @db.Timestamp(0)
  deleted_at          DateTime?         @db.Timestamp(0)
  proposal_comments   proposal_comments @relation(fields: [proposal_comment_id], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "proposal_comment_replies_proposal_comment_id_foreign")
  users               users             @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "proposal_comment_replies_user_id_foreign")

  @@index([proposal_comment_id, created_at], map: "proposal_comment_replies_proposal_comment_id_created_at_index")
  @@index([user_id], map: "proposal_comment_replies_user_id_index")
}

model proposal_comments {
  id                       BigInt                     @id @default(autoincrement()) @db.UnsignedBigInt
  sourcing_proposal_id     BigInt                     @db.UnsignedBigInt
  user_id                  BigInt                     @db.UnsignedBigInt
  comment                  String                     @db.Text
  created_at               DateTime?                  @db.Timestamp(0)
  updated_at               DateTime?                  @db.Timestamp(0)
  deleted_at               DateTime?                  @db.Timestamp(0)
  proposal_comment_replies proposal_comment_replies[]
  sourcing_proposals       sourcing_proposals         @relation(fields: [sourcing_proposal_id], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "proposal_comments_sourcing_proposal_id_foreign")
  users                    users                      @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "proposal_comments_user_id_foreign")

  @@index([sourcing_proposal_id, created_at], map: "proposal_comments_sourcing_proposal_id_created_at_index")
  @@index([user_id], map: "proposal_comments_user_id_index")
}

model role_has_permissions {
  permission_id BigInt      @db.UnsignedBigInt
  role_id       BigInt      @db.UnsignedBigInt
  permissions   permissions @relation(fields: [permission_id], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "role_has_permissions_permission_id_foreign")
  roles         roles       @relation(fields: [role_id], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "role_has_permissions_role_id_foreign")

  @@id([permission_id, role_id])
  @@index([role_id], map: "role_has_permissions_role_id_foreign")
}

model roles {
  id                   BigInt                 @id @default(autoincrement()) @db.UnsignedBigInt
